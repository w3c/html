  <dfn attribute for="HTMLObjectElement"><code>archive</code></dfn>,
  <dfn attribute for="HTMLObjectElement"><code>border</code></dfn>,
  <dfn attribute for="HTMLObjectElement"><code>code</code></dfn>,
